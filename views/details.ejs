<% layout("layouts/boilerplate")%>

    <h1 class="display-6 text-center">
        <%= title%>
    </h1>
    <br>
    <div class="row">
        <div class="col-6">
            <%- include("./partials/imageCarousel") %>
        </div>
        <div class="col-6">
            <iframe src="<%=map%>" style="width: 100%;height: 100%;"></iframe>
        </div>
    </div>

    <div class="container-fluid pt-3">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">About</h5>
                <p class="card-text">
                    <%=data.intro%>
                </p>
                <h5 class="card-title">Learn More!</h5>
                <h6 class="card-subtitle mb-2 text-muted">Expand each section below to learn more</h6>
                <div class="row">
                    <%- include("./partials/sections") %>
                        <%- include("./partials/references") %>
                </div>
            </div>
        </div>
    </div>